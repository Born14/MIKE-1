# MIKE-1 Environment Configuration
# Copy this to .env and fill in your values

# =============================================================================
# ROBINHOOD CREDENTIALS
# =============================================================================
ROBINHOOD_USERNAME=your_email@example.com
ROBINHOOD_PASSWORD=your_password

# Optional: Device token for 2FA (get this after first login)
# ROBINHOOD_DEVICE_TOKEN=

# =============================================================================
# DATABASE (NeonDB)
# =============================================================================
DATABASE_URL=postgresql://user:password@host/database?sslmode=require

# =============================================================================
# ENVIRONMENT
# =============================================================================
# Options: development, production
MIKE1_ENV=development

# Path to config file (optional, defaults to config/default.yaml)
# MIKE1_CONFIG=config/default.yaml
